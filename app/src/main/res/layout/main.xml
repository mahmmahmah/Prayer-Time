<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#FFFCF9"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear60"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<ProgressBar
				android:id="@+id/progressbar1"
				android:layout_width="25dp"
				android:layout_height="25dp"
				android:indeterminate="false"
				style="?android:progressBarStyle" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear2"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="30dp"
			android:paddingLeft="8dp"
			android:paddingRight="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview1"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:src="@drawable/ic_keyboard_arrow_left_black"
				android:scaleType="fitCenter" />
			<LinearLayout
				android:id="@+id/linear4"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:gravity="center_horizontal|top"
				android:orientation="vertical"
				android:layout_weight="1">
				<TextView
					android:id="@+id/textview2"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="7 Ramadan,1444"
					android:textSize="25sp"
					android:textStyle="bold"
					android:textColor="#22333B" />
				<LinearLayout
					android:id="@+id/linear5"
					android:layout_width="match_parent"
					android:layout_height="10dp"
					android:padding="8dp"
					android:orientation="horizontal" />
				<TextView
					android:id="@+id/textview1"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Wed, 29 March 2023"
					android:textSize="16sp"
					android:textColor="#616161" />
			</LinearLayout>
			<ImageView
				android:id="@+id/imageview2"
				android:layout_width="40dp"
				android:layout_height="40dp"
				android:src="@drawable/ic_chevron_right_black"
				android:scaleType="fitCenter" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear6"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<androidx.cardview.widget.CardView
				android:id="@+id/cardview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				app:cardElevation="1dp"
				app:cardCornerRadius="25dp">
				<LinearLayout
					android:id="@+id/linear8"
					android:layout_width="180dp"
					android:layout_height="165dp"
					android:background="@drawable/img1"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear12"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear13"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Now time is"
								android:textSize="18sp"
								android:textColor="#212121" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear14"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:paddingBottom="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/current_prayer_name_text_view"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Duhur"
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#F18F5C" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear15"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:paddingBottom="5dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/current_prayer_time_text_view"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingRight="3dp"
								android:text="12:27"
								android:textSize="30sp"
								android:textStyle="bold"
								android:textColor="#222222" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear16"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview7"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingRight="3dp"
								android:text="End time -"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="#424242" />
							<TextView
								android:id="@+id/textview8"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="2dp"
								android:text="3:54pm"
								android:textSize="12sp"
								android:textStyle="bold"
								android:textColor="#424242" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</androidx.cardview.widget.CardView>
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="10dp"
				android:layout_height="match_parent"
				android:padding="8dp"
				android:orientation="vertical" />
			<androidx.cardview.widget.CardView
				android:id="@+id/cardview2"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:orientation="vertical"
				app:cardElevation="1dp"
				app:cardCornerRadius="25dp">
				<LinearLayout
					android:id="@+id/linear11"
					android:layout_width="180dp"
					android:layout_height="165dp"
					android:background="@drawable/img2"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear17"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical">
						<LinearLayout
							android:id="@+id/linear18"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview9"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Next prayer is"
								android:textSize="18sp"
								android:textColor="#212121" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear19"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:paddingBottom="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/next_prayer_name_text_view"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Asr"
								android:textSize="20sp"
								android:textStyle="bold"
								android:textColor="#F18F5C" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear20"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:paddingBottom="5dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/next_prayer_time_text_view"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingRight="3dp"
								android:text="03:54"
								android:textSize="30sp"
								android:textStyle="bold"
								android:textColor="#222222" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear21"
							android:layout_width="match_parent"
							android:layout_height="20dp"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:gravity="top"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview13"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingRight="3dp"
								android:text="Azan -"
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="#424242" />
							<TextView
								android:id="@+id/azan_time_text_view"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:paddingLeft="2dp"
								android:text="5:15pm"
								android:textSize="12sp"
								android:textStyle="bold"
								android:textColor="#424242" />
						</LinearLayout>
						<LinearLayout
							android:id="@+id/linear22"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingLeft="8dp"
							android:paddingRight="8dp"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview15"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Jama'at - "
								android:textSize="14sp"
								android:textStyle="bold"
								android:textColor="#424242" />
							<TextView
								android:id="@+id/jamaat_time_text_view"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="5:30pm"
								android:textSize="12sp"
								android:textStyle="bold"
								android:textColor="#424242" />
						</LinearLayout>
					</LinearLayout>
				</LinearLayout>
			</androidx.cardview.widget.CardView>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear23"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:paddingLeft="15dp"
			android:paddingTop="8dp"
			android:paddingRight="15dp"
			android:paddingBottom="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear25"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:orientation="horizontal"
				android:layout_weight="1" />
			<LinearLayout
				android:id="@+id/linear24"
				android:layout_width="match_parent"
				android:layout_height="80dp"
				android:paddingTop="8dp"
				android:paddingBottom="8dp"
				android:background="#FAF3EE"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear26"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal"
					android:layout_weight="1">
					<LinearLayout
						android:id="@+id/linear27"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:gravity="right"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/linear30"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingRight="15dp"
							android:gravity="right|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview18"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Sunrise"
								android:textSize="14sp"
								android:textColor="#222222" />
						</LinearLayout>
						<TextView
							android:id="@+id/textview17"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingRight="15dp"
							android:text="04:57am"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear28"
						android:layout_width="2dp"
						android:layout_height="40dp"
						android:padding="8dp"
						android:background="#F18F5C"
						android:orientation="vertical" />
					<LinearLayout
						android:id="@+id/linear29"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:paddingLeft="15dp"
						android:orientation="vertical"
						android:layout_weight="1">
						<LinearLayout
							android:id="@+id/linear31"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:gravity="left|center_vertical"
							android:orientation="horizontal">
							<TextView
								android:id="@+id/textview19"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text="Midnight"
								android:textSize="14sp"
								android:textColor="#222222" />
						</LinearLayout>
						<TextView
							android:id="@+id/textview20"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="6:37pm"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="#000000" />
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear32"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:paddingLeft="15dp"
			android:paddingTop="8dp"
			android:paddingRight="15dp"
			android:paddingBottom="8dp"
			android:orientation="vertical">
			<LinearLayout
				android:id="@+id/linear33"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:padding="8dp"
				android:background="#FAF3EE"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/linear56"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<LinearLayout
						android:id="@+id/linear57"
						android:layout_width="100dp"
						android:layout_height="wrap_content"
						android:paddingRight="15dp"
						android:gravity="right|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<TextView
							android:id="@+id/country"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="Country"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="#000000" />
					</LinearLayout>
					<LinearLayout
						android:id="@+id/linear58"
						android:layout_width="2dp"
						android:layout_height="40dp"
						android:padding="8dp"
						android:background="#F18F5C"
						android:orientation="vertical" />
					<LinearLayout
						android:id="@+id/linear59"
						android:layout_width="100dp"
						android:layout_height="wrap_content"
						android:paddingLeft="15dp"
						android:gravity="left|center_vertical"
						android:orientation="vertical"
						android:layout_weight="1">
						<TextView
							android:id="@+id/city"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:text="City"
							android:textSize="18sp"
							android:textStyle="bold"
							android:textColor="#000000" />
					</LinearLayout>
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear34"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/fajrTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Fajr"
						android:textSize="18sp"
						android:textColor="#212121" />
					<LinearLayout
						android:id="@+id/linear35"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"
						android:layout_weight="1" />
					<TextView
						android:id="@+id/fajrTimeTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="TextView"
						android:textSize="16sp"
						android:textColor="#000000" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear36"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#F18F5C"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/linear37"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/dhuhrTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Dhuhr"
						android:textSize="18sp"
						android:textColor="#212121" />
					<LinearLayout
						android:id="@+id/linear38"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"
						android:layout_weight="1" />
					<TextView
						android:id="@+id/dhuhrTimeTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="TextView"
						android:textSize="16sp"
						android:textColor="#000000" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear39"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#F18F5C"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/linear40"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/asrTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Asr"
						android:textSize="18sp"
						android:textColor="#212121" />
					<LinearLayout
						android:id="@+id/linear41"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"
						android:layout_weight="1" />
					<TextView
						android:id="@+id/asrTimeTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="TextView"
						android:textSize="16sp"
						android:textColor="#000000" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear42"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#F18F5C"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/linear43"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/maghribTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Maghrib"
						android:textSize="18sp"
						android:textColor="#212121" />
					<LinearLayout
						android:id="@+id/linear44"
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:padding="8dp"
						android:orientation="vertical"
						android:layout_weight="1" />
					<TextView
						android:id="@+id/maghribTimeTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:text="TextView"
						android:textSize="16sp"
						android:textColor="#000000" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear45"
					android:layout_width="match_parent"
					android:layout_height="1dp"
					android:padding="8dp"
					android:background="#F18F5C"
					android:orientation="horizontal" />
				<LinearLayout
					android:id="@+id/linear46"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="horizontal">
					<TextView
						android:id="@+id/ishaTextView"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
